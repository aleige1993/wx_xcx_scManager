<!--pages/platform/warehous/getInfo.wxml-->
<view>
<form bindsubmit="formSubmit">
    <van-cell-group>
      <van-field
        value="{{ company.expressNo}}"
        label="快递单号"
        placeholder="请输入快递单号"
        input-align="right"
        name="expressNo"
      />
      <van-field
        value="{{ company.mobile }}"
        label="手机号"
        input-align="right"
        placeholder="请输入手机号"
        name="mobile"
      />
      <van-cell
        title="入库单号"
        title-width="140rpx"
        value="{{company.orderNo}}"
      />
      <van-cell
        title="快递公司"
        title-width="140rpx"
        value="{{company.company}}"
      />
      <van-cell
        title="入库操作"
        title-width="140rpx"
        value="{{userInfo.userName}}"
      />
      <van-cell
        title="入库地址"
        title-width="140rpx"
        value="{{company.stationAddr}}"
      />
       <van-cell
        title="入库时间"
        title-width="140rpx"
        value="{{company.gmtCreate}}"
      />
    </van-cell-group>

    <view class='marig-primary'>
        <button class="button" form-type='submit' size="normal" >提交</button>
    </view>
    </form>
</view>
