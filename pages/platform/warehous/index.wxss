/* pages/library/warehous/index.wxss */
.clickAuto{
  text-align: right;
  width: 95%;
}
.waiter{
    display: flex;
    background-color: #fff;
    padding: 10rpx 30rpx;
    padding-right: 0;

    justify-content: space-between;
   align-content: center;
   border-bottom: 1px solid #eee;
}
.waiter view{
    display: flex;
   align-items: center;
   align-content: center;
   color: #666;
}
.waiter view icon{
    margin-bottom: 10rpx;
    margin-right: 10rpx;
}

.scan-code {
  width: 100%;
  height: 400rpx;
}
.retrieval_panl{
    position: relative;
}
.retrieval_box{
    position: absolute;
    z-index: 1;
    top: 0;
    left: 2%;
    width: 96%;
    background-color: #fff;
    padding: 0 20rpx;
}
.title_panl{
    display: flex;
    justify-content: space-between;
    color: #999;
    padding: 20rpx 20rpx 0 20rpx;
}
.retrList view{
    padding: 20rpx;
    border-bottom: 1px solid #eee;
}
.retrList{
    max-height:500rpx;
    overflow-y: auto;
}
